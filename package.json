{
  "name": "ssb-db2-box2",
  "description": "Box2 support for SSB DB2",
  "version": "0.6.0",
  "homepage": "https://github.com/ssb-ngi-pointer/ssb-db2-box2",
  "repository": {
    "type": "git",
    "url": "git@github.com:ssb-ngi-pointer/ssb-db2-box2.git"
  },
  "files": [
    "*.js",
    "package.json.license",
    "LICENSES/*"
  ],
  "dependencies": {
    "envelope-js": "^1.1.0",
    "p-defer": "^3.0.0",
    "private-group-spec": "^1.0.0",
    "ssb-bendy-butt": "^0.12.1",
    "ssb-bfe": "^3.1.1",
    "ssb-private-group-keys": "^0.4.0",
    "ssb-ref": "^2.16.0",
    "ssb-uri2": "^1.7.2"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "husky": "^4.3.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "pull-stream": "^3.6.14",
    "rimraf": "^3.0.2",
    "secret-stack": "6.3.2",
    "ssb-backlinks": "^2.1.1",
    "ssb-caps": "1.1.0",
    "ssb-db": "^20.4.0",
    "ssb-db2": "^2.7.0",
    "ssb-query": "^2.4.5",
    "ssb-tribes": "^2.2.0",
    "tap-arc": "^0.3.4",
    "tape": "^5.2.2"
  },
  "scripts": {
    "format-code": "prettier --write \"*.js\" \"test/*.js\"",
    "format-code-staged": "pretty-quick --staged --pattern \"*.js\" --pattern \"(test|compat|indexes|operators)/*.js\"",
    "test": "tape test/*.js | tap-arc",
    "coverage": "c8 --reporter=lcov npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format-code-staged"
    }
  },
  "author": "Anders Rune Jensen <arj03@protonmail.ch>",
  "license": "AGPL-3.0"
}
